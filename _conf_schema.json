{
    "api_url": {
        "description": "API的请求地址",
        "type": "string",
        "hint": "千问视频生成服务的API接口地址",
        "default": "https://ai.gitee.com/v1/async/videos/generations",
        "obvious_hint": false
    },
    "task_url": {
        "description": "任务查询地址",
        "type": "string",
        "hint": "用于查询视频生成任务状态的接口地址",
        "default": "https://ai.gitee.com/v1/task",
        "obvious_hint": false
    },
    "api_key": {
        "description": "API密钥",
        "type": "string",
        "hint": "请填写千问API服务提供的密钥，代码会自动添加'Bearer '前缀",
        "default": "",
        "obvious_hint": false
    },
    "model": {
        "description": "视频生成模型名称",
        "type": "string",
        "hint": "千问视频生成模型，推荐使用 Wan2.1-T2V-1.3B",
        "default": "Wan2.1-T2V-1.3B",
        "obvious_hint": false
    },
    "num_inference_steps": {
        "description": "推理步数",
        "type": "integer",
        "hint": "控制生成质量，步数越多质量越好但耗时越长，建议30-50",
        "default": 50,
        "obvious_hint": false
    },
    "num_frames": {
        "description": "视频帧数",
        "type": "integer",
        "hint": "生成视频的总帧数，81帧约3-4秒，值越大视频越长",
        "default": 81,
        "obvious_hint": false
    },
    "negative_prompt": {
        "description": "负面提示词",
        "type": "string",
        "hint": "指导模型避免生成的内容，例如：模糊、低质量、变形等",
        "default": "",
        "obvious_hint": false
    },
    "timeout": {
        "description": "任务超时时间(秒)",
        "type": "integer",
        "hint": "视频生成任务的最长等待时间，超时将取消任务",
        "default": 1800,
        "obvious_hint": false
    },
    "retry_interval": {
        "description": "状态查询间隔(秒)",
        "type": "integer",
        "hint": "每隔多少秒查询一次任务状态",
        "default": 10,
        "obvious_hint": false
    }
}