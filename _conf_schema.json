{
    "api_key": {
        "description": "API密钥",
        "type": "string",
        "hint": "请填写千问API服务提供的密钥，代码会自动添加'Bearer '前缀",
        "default": "",
        "obvious_hint": false
    },
    "image_to_video_url": {
        "description": "图生视频API地址",
        "type": "string",
        "hint": "千问图生视频API接口地址，如果接口不支持请修改此地址",
        "default": "https://ai.gitee.com/v1/async/videos/image-to-video",
        "obvious_hint": false
    },
    "task_url": {
        "description": "任务查询地址",
        "type": "string",
        "hint": "用于查询视频生成任务状态的接口地址",
        "default": "https://ai.gitee.com/v1/task",
        "obvious_hint": false
    },
    "model": {
        "description": "视频生成模型名称",
        "type": "string",
        "hint": "千问图生视频模型，推荐使用 Wan2.1-I2V-14B-720P",
        "default": "Wan2.1-I2V-14B-720P",
        "obvious_hint": false
    },
    "num_inference_steps": {
        "description": "推理步数",
        "type": "integer",
        "hint": "控制生成质量，步数越多质量越好但耗时越长，建议30-50",
        "default": 50,
        "obvious_hint": false
    },
    "num_frames": {
        "description": "视频帧数",
        "type": "integer",
        "hint": "生成视频的总帧数，81帧约3-4秒，值越大视频越长",
        "default": 81,
        "obvious_hint": false
    },
    "aspect_ratio": {
        "description": "图生视频宽高比",
        "type": "string",
        "hint": "视频的宽高比，可选值：9:16(竖版), 16:9(横版), 1:1(方形)",
        "default": "16:9",
        "obvious_hint": false
    },
    "orientation": {
        "description": "图生视频方向",
        "type": "string",
        "hint": "视频方向，可选值：landscape(横屏), portrait(竖屏), square(方形)",
        "default": "landscape",
        "obvious_hint": false
    }
}
